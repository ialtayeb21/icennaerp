  "EwbDtls": {
    "type": "object",
    "properties": {
      "TransId": {
        "type": "string",
        "minLength": 15,
        "maxLength": 15
      },
      "TransName": {
        "type": "string",
        "minLength": 3,
        "maxLength": 100
      },
      "TransMode": {
        "type": "string",
        "maxLength": 1,
        "minLength": 1,
        "enum": ["1", 2, 3, 4]
      },
      "Distance": {
        "type": "number",
        "minimum": 1,
        "maximum": 9999
      },
      "TransDocNo": {
        "type": "string",
        "minLength": 1,
        "maxLength": 15,
