        "maxLength": 18
      },
      "FinInsBr": {
        "type": "string",
        "minLength": 1,
        "maxLength": 11
      },
      "PayTerm": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "PayInstr": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "CrTrn": {
        "type": "string",
        "minLength": 1,
