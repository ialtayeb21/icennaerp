      },
      "Addr2": {
        "type": "string",
        "minLength": 3,
        "maxLength": 100
      },
      "Loc": {
        "type": "string",
        "minLength": 3,
        "maxLength": 100
      },
      "Pin": {
        "type": "number",
        "minimum": 100000,
        "maximum": 999999
      },
      "Stcd": {
        "type": "string",
        "minLength": 1,
        "maxLength": 2
      }
    },
    "required": ["LglNm", "Addr1", "Loc", "Pin", "Stcd"]
  },
